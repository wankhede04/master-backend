{
  "name": "liquidation-bot",
  "version": "0.0.56",
  "description": "Liquidation bot for perpetual",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "ts-node": "node -r ts-node/register/transpile-only src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/config/postgreTypeorm.ts",
    "typeorm:prod": "node ./node_modules/typeorm/cli.js --config dist/config/postgreTypeorm.js",
    "typeorm:migrate": "npm run typeorm -- migration:run",
    "typeorm:migrate:prod": "npm run typeorm:prod -- migration:run",
    "typeorm:migrate:undo": "npm run typeorm -- migration:revert",
    "typeorm:migrate:prod:undo": "npm run typeorm:prod -- migration:revert",
    "typeorm:seed": "ts-node ./node_modules/typeorm/cli.js --config src/config/postgreSeedTypeorm.ts migration:run",
    "typeorm:seed:undo": "ts-node ./node_modules/typeorm/cli.js --config src/config/postgreSeedTypeorm.ts migration:revert",
    "typeorm:seed:prod": "node ./node_modules/typeorm/cli.js --config dist/config/postgreSeedTypeorm.js migration:run",
    "typeorm:seed:prod:undo": "node ./node_modules/typeorm/cli.js --config dist/config/postgreSeedTypeorm.js migration:revert",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "preversion": "npm run build:check",
    "version": "standard-version",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.812.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.3",
    "express-validation": "^3.0.6",
    "googleapis": "^66.0.0",
    "faker": "^5.1.0",
    "helmet": "^4.2.0",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pm2": "^4.5.1",
    "puppeteer-core": "^5.5.0",
    "rand-token": "^1.0.1",
    "rate-limit-redis": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "slug": "^4.0.2",
    "typeorm": "0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-slack-webhook-transport": "^2.0.0",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/express-rate-limit": "^5.1.0",
    "@types/faker": "^5.1.5",
    "@types/helmet": "0.0.48",
    "@types/ioredis": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.14",
    "@types/nodemailer": "^6.4.0",
    "@types/puppeteer-core": "^2.0.0",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.10",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
